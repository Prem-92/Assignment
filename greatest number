#include <stdio.h>
int main()
{
    int a,b,c;
    printf("enter first value:");
    scanf("%d",&a);
    printf("enter second value:");
    scanf("%d",&b);
    printf("enter third value:");
    scanf("%d",&c);
    if(a>c&&a>b)
    {
        printf("%d is the greatest number is",a);
    }
    else if(b>c&&b>a)
    {
        printf("%d is the greatest number is",b);
    }
    else
    {
        printf("%d is the greatest number is",c);
    }
    return 0;
}
